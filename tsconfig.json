{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "alwaysStrict": false,
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "target": "es2015",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": []
  },
  "include": [
    "./test/fixtures/*.js",
  ],
}
